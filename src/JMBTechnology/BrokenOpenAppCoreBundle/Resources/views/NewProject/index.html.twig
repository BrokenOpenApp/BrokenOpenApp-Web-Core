{% extends 'JMBTechnologyBrokenOpenAppCoreBundle::layout.html.twig' %}


{% block main %}

	<section class="issue-header">
		<h1>New Project</h1>
	</section>

	<section class="issue-crashes">
		<form class="styled" action="" method="post" {{ form_enctype(form) }}>
			{{ form_errors(form) }}

			{{ form_row(form.title) }}

			{{ form_rest(form) }}

			<input type="submit" value="Create Project" />
		</form>

	</section>

{% endblock %}

