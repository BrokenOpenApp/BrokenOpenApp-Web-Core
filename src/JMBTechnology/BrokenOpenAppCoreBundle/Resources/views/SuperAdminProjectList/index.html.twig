{% extends 'JMBTechnologyBrokenOpenAppCoreBundle::layout.html.twig' %}





{% block main %}

	<section class="latest-issues">
		<h2>Projects</h2>
		{% if projects %}
			<table class="table table-striped">
				<thead>
					<tr>
						<th>ID</th>
						<th>Title</th>
						<th></th>
					</tr>
				</thead>
				<tbody>
					{% for project in projects %}
						<tr>
							<td>{{ project.id }}</td>
							<td>{{ project.title }}</td>
							<td class="action-link">&nbsp;</td>
						</tr>
					{% endfor %}
				</tbody>
			</table>
		{% else %}
			<p>No projects.</p>
		{% endif %}
	</section>



{%  endblock %}


